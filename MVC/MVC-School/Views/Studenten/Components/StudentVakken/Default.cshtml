@model IEnumerable<MVC_School.Models.VakStudent>

<h4>StudentVakken</h4>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Student)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vak)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Uren)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Student.ID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Vak.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Uren)
                </td>
            </tr>
        }
    </tbody>
</table>